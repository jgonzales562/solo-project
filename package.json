{
  "name": "solo-project",
  "version": "1.0.0",
  "private": true,
  "description": "Interactive middleware composer for Express.js - visualize, test, and export middleware chains",
  "main": "dist/server.js",
  "scripts": {
    "test": "npm run build && npm run lint && npm run test:integration",
    "test:integration": "node scripts/run-integration.mjs",
    "lint:ci": "npm run lint -- --max-warnings=0",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"scripts/**/*.mjs\" \"client/**/*.{js,html,css}\"",
    "ci": "npm run format:check && npm run lint:ci && npm run build && npm run test:integration",
    "build": "tsc",
    "clean": "node -e \"const fs=require('fs');fs.rmSync('dist',{recursive:true,force:true});fs.rmSync('.tmp-test',{recursive:true,force:true});\"",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "node scripts/dev-client.mjs",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "lint": "eslint src/ client/ scripts/ test/",
    "lint:fix": "eslint src/ client/ scripts/ test/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"scripts/**/*.mjs\" \"client/**/*.{js,html,css}\""
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgonzales562/solo-project.git"
  },
  "keywords": [
    "express",
    "middleware",
    "composer",
    "testing",
    "visualization",
    "typescript"
  ],
  "author": "jgonzales562",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jgonzales562/solo-project/issues"
  },
  "homepage": "https://github.com/jgonzales562/solo-project#readme",
  "dependencies": {
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.3",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
